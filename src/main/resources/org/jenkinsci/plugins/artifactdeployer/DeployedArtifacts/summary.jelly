<j:jelly xmlns:j="jelly:core"
         xmlns:t="/lib/hudson">
    <t:summary icon="package.gif">
        ${%Deployed Artifacts}
        <ul>
            <j:forEach var="artifact" items="${it.allArtifacts}">
                <li>
                    <a href="deployedArtifacts/download/artifact.${artifact.id}/">${h.escape(artifact.fileName)}</a>
                    <br/>
                </li>
            </j:forEach>
        </ul>
    </t:summary>
</j:jelly>